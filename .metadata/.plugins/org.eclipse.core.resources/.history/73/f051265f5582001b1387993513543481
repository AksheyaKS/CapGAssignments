package labSix;

import java.util.Scanner;

public class NumberSquares {

	public static void main(String[] args) {
		
		Scanner s = new Scanner(System.in);
		System.out.println("Enter the number of elements: ");
		int n = s.nextInt();
		
		int arr [] = new int[n];
		
		for(int i = 0; i < n; i++) {
			
			System.out.println("Enter element: ");
			arr[i] = s.nextInt();
			
		}
		
		

	}

}
